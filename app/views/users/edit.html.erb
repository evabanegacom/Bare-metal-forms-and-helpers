<%= form_for @user, { action: 'update' } do |f| %>
  <%= f.label :username %><br><br>
  <%= f.text_field :username, value: @user.username %><br><br>
  <%= f.label :email %><br><br>
  <%= f.text_field :email, value: @user.email %><br><br>
  <%= f.label :password%><br><br>
  <%= f.text_field :password, value: @user.password %><br><br>
  <%= f.submit 'submit'%>
<% end %>